<launch>	  
	<!-- urdf xml robot description loaded on the Parameter Server, converting the xacro into a proper urdf file-->
	<param name="robot_description" command="xacro '$(find my_robot_description)/urdf/my_robot.xacro'" />

	<!-- push robot_description to factory and spawn robot in gazebo -->
	<node name="my_robot_spawner" pkg="gazebo_ros" type="spawn_model" output="screen"
	  args="-urdf -param robot_description -model my_robot" />

	<include file="$(find gazebo_ros)/launch/empty_world.launch">	    
		<!-- <arg name="world_name" value="$(find mybot_gazebo)/worlds/mybot.world"/>	     -->
		<arg name="world_name" value="worlds/mud.world"/>
		<arg name="gui" value="true"/>	  
	</include>	
</launch>
